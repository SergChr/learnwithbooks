<% include _header %>

<div class="booksBox">
    
    <div class="menu">
    <% include _menubar %>
    <i class="fa fa-2x fa-bars bars" aria-hidden="true"></i>
    </div>
        
     <% for (var i = 0; i<books.length; i++) { %>
<div class="book">
   
    <div class="img">
     <font class="title"> <%= books[i]["title"] %> </font>
     <img src="<%= books[i]['imageSource'] %>">
     </div>
    <button class="read" data-value="<%= books[i]['bookSource'] %>" >Read</button>
    </div>
    <% } %>
</div>

<script src="/js/books.js"></script>
    <% include _footer %>